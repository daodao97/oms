{
  "name": "@okiss/vbtf",
  "version": "0.0.86",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "cross-env pkg=vbtf vite --config ../../build/dev.config.ts",
    "build": "cross-env pkg=vbtf vite build --config ../../build/lib.config.ts",
    "ts": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck",
    "release": "cross-env pkg=vbtf node ../../build/release.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "@okiss/utils": "latest",
    "axios": "^1.7.7",
    "dayjs": "^1.11.13",
    "element-plus": "2.8.1",
    "file-saver": "^2.0.5",
    "inputmask": "^5.0.9",
    "lodash": "^4.17.21",
    "sortablejs": "^1.15.3",
    "vue": "^3.4.38",
    "vue-router": "^4.4.3",
    "vuedraggable": "^4.0.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitejs/plugin-vue-jsx": "^4.0.1",
    "@vue/compiler-sfc": "^3.5.6",
    "eslint": "^9.10.0",
    "eslint-plugin-vue": "^9.28.0",
    "monaco-editor": "^0.51.0",
    "sass": "^1.79.1",
    "typescript": "^5.6.2",
    "vite": "^5.4.6"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./src/index.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      "./style.css": "./dist/style.css",
      ".": {
        "import": "./dist/index.es.js"
      }
    }
  }
}