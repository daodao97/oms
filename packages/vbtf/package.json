{
  "name": "@okiss/vbtf",
  "version": "0.0.131",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "cross-env pkg=vbtf vite --config ../../build/dev.config.mts",
    "build": "cross-env pkg=vbtf vite build --config ../../build/lib.config.mts",
    "ts": "pnpm -w exec vue-tsc -p tsconfig.build.json",
    "release": "cross-env pkg=vbtf node ../../build/release.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.13.2",
    "dayjs": "^1.11.19",
    "file-saver": "^2.0.5",
    "inputmask": "^5.0.9",
    "lodash": "^4.17.21",
    "sortablejs": "^1.15.6",
    "vuedraggable": "^4.1.0",
    "xlsx": "^0.18.5"
  },
  "peerDependencies": {
    "@okiss/utils": ">=0.0.12",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "element-plus": "^2.11.9"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.2",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/compiler-sfc": "^3.5.25",
    "eslint": "^9.39.1",
    "eslint-plugin-vue": "^10.6.2",
    "monaco-editor": "^0.55.1",
    "sass": "^1.94.2",
    "typescript": "^5.9.3",
    "vite": "^7.2.6",
    "vue-tsc": "^3.1.5"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./src/index.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      "./style.css": "./dist/style.css",
      ".": {
        "import": "./dist/index.es.js"
      }
    }
  }
}