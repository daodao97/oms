{
  "name": "@okiss/oms",
  "version": "0.0.73",
  "scripts": {
    "dev": "cross-env pkg=oms vite --config ../../build/dev.config.mts",
    "build": "cross-env pkg=oms vite build --config ../../build/lib.config.mts",
    "ts": "pnpm -w exec vue-tsc -p tsconfig.build.json",
    "release": "cross-env pkg=oms node ../../build/release.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^11.3.0",
    "axios": "^1.12.1",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.18",
    "element-plus": "^2.11.3",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "monaco-editor": "^0.51.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^8.3.0",
    "pinia": "^2.3.1",
    "qs": "^6.14.0",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1",
    "vue3-markdown-it": "^1.0.10",
    "vuedraggable": "^4.1.0"
  },
  "peerDependencies": {
    "@okiss/utils": ">=0.0.12",
    "@okiss/vbtf": ">=0.0.110",
    "element-plus": "^2.11.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./src/index.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      "./style.css": "./dist/style.css",
      ".": {
        "import": "./dist/index.es.js"
      }
    }
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vue-tsc": "^2.2.12"
  }
}