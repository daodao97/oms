{
  "name": "@okiss/oms",
  "version": "0.0.93",
  "scripts": {
    "dev": "cross-env pkg=oms vite --config ../../build/dev.config.mts",
    "prebuild": "pnpm run update:peers",
    "build": "cross-env pkg=oms vite build --config ../../build/lib.config.mts",
    "ts": "pnpm -w exec vue-tsc -p tsconfig.build.json",
    "release": "cross-env pkg=oms node ../../build/release.js",
    "update:peers": "node ./scripts/update-peers.mjs"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "@vueuse/core": "^14.1.0",
    "axios": "^1.13.2",
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.19",
    "element-plus": "^2.11.9",
    "js-cookie": "^3.0.5",
    "lodash": "^4.17.21",
    "mitt": "^3.0.1",
    "mockjs": "^1.1.0",
    "monaco-editor": "^0.55.1",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "path-to-regexp": "^8.3.0",
    "pinia": "^3.0.4",
    "qs": "^6.14.0",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "vue3-markdown-it": "^1.0.10",
    "vuedraggable": "^4.1.0"
  },
  "peerDependencies": {
    "@okiss/utils": ">=0.0.37",
    "@okiss/vbtf": ">=0.0.131",
    "element-plus": "^2.11.9",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./src/index.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      "./style.css": "./dist/style.css",
      ".": {
        "import": "./dist/index.es.js"
      }
    }
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.5"
  }
}
