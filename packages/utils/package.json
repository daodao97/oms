{
  "name": "@okiss/utils",
  "version": "0.0.35",
  "scripts": {
    "build": "cross-env pkg=utils vite build --config ../../build/lib.config.mts",
    "build:report": "cross-env pkg=utils EXPORT=true vite build --config ../../build/lib.config.mts",
    "dev": "cross-env pkg=utils vite --config ../../build/dev.config.mts",
    "ts": "pnpm -w exec vue-tsc -p tsconfig.build.json",
    "release": "cross-env pkg=utils node ../../build/release.js"
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "element-plus": "^2.11.9",
    "handlebars": "^4.7.8",
    "jsonpath": "^1.1.1",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "typescript": "^5.9.3",
    "vue-tsc": "^3.1.5"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./src/index.ts",
  "publishConfig": {
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts"
  }
}