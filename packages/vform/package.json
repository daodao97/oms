{
  "name": "@okiss/vform",
  "version": "0.0.60",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "cross-env pkg=vform vite --config ../../build/dev.config.ts",
    "build": "cross-env pkg=vform vite build --config ../../build/lib.config.ts",
    "export": "cross-env EXPORT=true pkg=vform vite build --config ../../build/lib.config.ts",
    "ts": "vue-tsc --declaration --emitDeclarationOnly --skipLibCheck",
    "release": "cross-env pkg=vform node ../../build/release.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "@okiss/utils": "latest",
    "axios": "^1.4.0",
    "dayjs": "^1.11.7",
    "element-plus": "2.3.5",
    "inputmask": "^5.0.8",
    "lodash": "^4.17.21",
    "vue": "^3.3.4",
    "vuedraggable": "^4.0.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "@vue/compiler-sfc": "^3.2.45",
    "eslint": "^8.27.0",
    "eslint-plugin-vue": "^9.7.0",
    "sass": "^1.56.1",
    "typescript": "^4.8.4",
    "vite": "^3.2.3"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./src/index.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      "./style.css": "./dist/style.css",
      ".": {
        "import": "./dist/index.es.js"
      }
    }
  }
}