{
  "name": "@okiss/vform",
  "version": "0.0.64",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "cross-env pkg=vform vite --config ../../build/dev.config.mts",
    "build": "cross-env pkg=vform vite build --config ../../build/lib.config.mts",
    "export": "cross-env EXPORT=true pkg=vform vite build --config ../../build/lib.config.mts",
    "ts": "pnpm -w exec vue-tsc -p tsconfig.build.json",
    "release": "cross-env pkg=vform node ../../build/release.js"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "axios": "^1.12.1",
    "dayjs": "^1.11.18",
    "inputmask": "^5.0.9",
    "lodash": "^4.17.21",
    "vuedraggable": "^4.1.0"
  },
  "peerDependencies": {
    "@okiss/utils": ">=0.0.12",
    "vue": "^3.5.21",
    "element-plus": "2.8.3"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitejs/plugin-vue-jsx": "^5.0.0",
    "@vue/compiler-sfc": "^3.5.6",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "eslint": "^9.35.0",
    "eslint-plugin-vue": "^9.33.0",
    "globals": "^15.8.0",
    "sass": "^1.92.1",
    "typescript": "^5.9.2",
    "vite": "^7.0.0",
    "vue-tsc": "^2.2.12"
  },
  "files": [
    "dist/*"
  ],
  "module": "./src/index.ts",
  "style": "./dist/style.css",
  "types": "./src/index.ts",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./src/index.ts"
    }
  },
  "publishConfig": {
    "module": "./dist/index.es.js",
    "types": "./dist/types/index.d.ts",
    "exports": {
      "./style.css": "./dist/style.css",
      ".": {
        "import": "./dist/index.es.js"
      }
    }
  }
}